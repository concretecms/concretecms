			<key />
			<unsigned />
		</field>
		<field name="fID" type="I">
			<unsigned />
			<default value="0" />
		</field>
		<field name="fOnstateID" type="I">
			<unsigned />
			<default value="0" />
		</field>
		<field name="maxWidth" type="I">
			<unsigned />
			<default value="0" />
		</field>
		<field name="maxHeight" type="I">
			<unsigned />
			<default value="0" />
		</field>
		<field name="externalLink"  type="C" size="255"/>
		<field name="internalLinkCID"  type="I">
			<unsigned />
			<default value="0" />
