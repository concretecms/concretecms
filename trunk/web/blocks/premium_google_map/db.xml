<?xml version="1.0"?>
<schema version="0.3">
	<table name="btGoogleMapPremium">
		<field name="bID" type="I">
			<key />
			<unsigned />
		</field>
		<field name="title" type="C" size="255"></field>		
		<field name="api_key" type="C" size="255"></field> 
		<field name="kml_fID" type="I"></field>
		<field name="w" type="C" size="8"></field>	
		<field name="h" type="C" size="8"></field>	
		<field name="map_type" type="C" size="32"></field>
		<field name="show_earth" type="I" size="2"><default value="0"/></field>	
		<field name="latitude" type="F"></field>
		<field name="longitude" type="F"></field>	
		<field name="zoom" type="I" size="8"></field>				
	</table>
</schema>